{
  "manifest_version": 3,
  "name": "Extension",
  "description": "Jaaj aberrant",
  "version": "1.0",
  "action": {
    "default_popup": "jaaj.html",
    "default_icon": "GMK.jpg"
  },
  "content_scripts": [
    {
      "matches": ["https://*.instagram.com/*"],
      "css": ["miteux.css"],
      "js": ["aberrant.js"],
      "all_frames": false,
      "match_origin_as_fallback": false,
      "match_about_blank": false,
      "run_at": "document_idle",
      "world": "ISOLATED"
    }
  ]
}
